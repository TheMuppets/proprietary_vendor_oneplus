               <!-- The below list is only for reference to enable/disable ICD per SSID basis
               <ICD config-type="disabled">
                  <apIds Type="SSID">
                     <apId>wifi-ap1</apId>
                     <apId>wifi-ap2</apId>
                  </apIds>
               </ICD>
